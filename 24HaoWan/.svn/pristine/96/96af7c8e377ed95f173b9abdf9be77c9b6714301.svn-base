*{ margin: 0;padding: 0; font-family: "Microsoft YaHei";}
li{ list-style: none;}
a{ color: #000; text-decoration: none;}
a:hover{ color:#fff;}
img{ border:0;}

.title{ font-size: 28px;}

#header-contain{ background: #fff; width: 100%;}
#header{ width: 960px; background: #fff; margin: 0 auto; height: 100px; position: relative;}
#logo{ float:left; display: block; width: 170px; height: 64px; margin: 18px 0px 18px 32px;}
#header-right{ float: right; width: 448px; height: 100px; font-size: 14px; }
#ul-container{ text-align: center;}
#ul-container ul li{ float:left;}
#ul-container ul li a{ width: 112px; height: 100px; line-height: 100px; display: block;}
.li-1{ background: #d04759;}
.li-1 a{ color: #fff;}
.li-1:hover{ background: #d04759;}
.li-2:hover{ background: #5557a0;}
.li-3:hover{ background: #fec356;}
.li-4:hover{ background: #54c0d6;}

#banner-contain{ width: 100%; height: 650px; background: #d04759; position: relative;}
/*#point{ width: 84px; position: absolute; z-index: 1; left:50%; margin-left: -42px; margin-top: -42px;}
#point li{ width: 9px; height: 9px; background: #d04759; float: left; margin:0 5px; border-radius: 10px; border: #fff 1px solid; cursor: pointer;}
#point .active{ width: 9px; height: 9px; background: #fff; float: left; margin:0 5px; border-radius: 10px; border: #fff 1px solid; }*/

#cases{ width: 960px; height: 390px; margin: 0 auto; margin-top: 50px; text-align: center;}
#cases-contain{ margin-top: 50px;}
.case{ float: left; width: 314px;}
.case img{ width: 314px;}
.case2{margin: 0 9px;}
.case-name{ margin: 16px 0; font-size: 14px;}
.case-slogan{ font-size: 12px; font-family: '宋体'}

#partner{ width: 960px; height: 248px; margin: 0 auto; text-align: center;}
#partner-contain{ margin-top: 50px;}
.partner{ float: left; width: 314px;}
.partner img{ width: 314px;}
.partner2{margin: 0 9px;}

#footer{ width: 100%; height: 171px;}
#code-contain{ width: 100%; margin: 0 auto; background: #2a2d30; height: 131px; position: relative;}
#code-contain img{ position: absolute; left: 50%; top: 50%; margin-left: -50px; margin-top: -50px;}
#footer-info{ width: 100%; margin: 0 auto; background: #000; height: 40px;}

/*#img-contain{ width: 8000px; height: 650px; overflow: hidden;}
#img-contain ul li{ float: left;}*/
#img-contain{ min-width: 1000px; height: 650px; position: relative; overflow: hidden;}
#img-contain ul{ position: absolute; left: 0; /*width: 8000px;*/}
#img-contain ul li{ float: left;}
#img-contain ul li img{ position: relative; left: 0;}

#btn{ position: absolute; width: 100%; text-align: center; bottom: 0;}
#btn a{ cursor: pointer; display: inline-block; width: 8px; height: 8px; border: #fff 1px solid; border-radius: 8px; position: relative; bottom: 30px;}
#btn a.active{ background: #fff;}
#btn a:hover{ background: #fff;}