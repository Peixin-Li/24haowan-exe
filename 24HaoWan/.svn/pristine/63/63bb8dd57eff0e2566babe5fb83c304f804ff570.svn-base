*{ margin: 0;padding: 0; font-family: "Microsoft YaHei"; font-weight: normal;}
li{ list-style: none;}
a{ color: #000; text-decoration: none;}
a:hover{ color:#fff;}
img{ border:0;}
/*html,body{ height: 100%;}*/

.title{ font-size: 28px;}

#header-contain{ background: #fff; width: 100%;}
#header{ width: 960px; background: #fff; margin: 0 auto; height: 100px; position: relative;}
#logo{ float:left; display: block; width: 170px; height: 64px; margin: 18px 0px 18px 32px;}
#header-right{ float: right; width: 448px; height: 100px; font-size: 14px; }
#ul-container{ text-align: center;}
#ul-container ul li{ float:left;}
#ul-container ul li a{ width: 112px; height: 100px; line-height: 100px; display: block;}
.li-3{ background: #fec356;}
.li-3 a{ color: #fff;}
.li-1:hover{ background: #d04759;}
.li-2:hover{ background: #5557a0;}
.li-3:hover{ background: #fec356;}
.li-4:hover{ background: #54c0d6;}

#game-custom{ width: 960px; margin: 0 auto; text-align: center; margin-top: 50px;}
#phone-contain{ margin-top: 50px; width: 960px; height: 680px;}
#phone-left{ width: 480px; height: 680px; background: url('../images/left-phone.png') no-repeat 50% 0; float: left;}
#phone-right{ width: 480px; height: 680px; background: url('../images/right-phone.png') no-repeat 50% 0; float: right;}
#phone-right a{ display: block; width: 141px; height: 47px; position: relative; left: 164px; top: 466px; }

#footer{ width: 100%; height: 171px;}
#code-contain{ width: 100%; margin: 0 auto; background: #2a2d30; height: 131px; position: relative;}
#code-contain img{ position: absolute; left: 50%; top: 50%; margin-left: -50px; margin-top: -50px;}
#footer-info{ width: 100%; margin: 0 auto; background: #000; height: 40px;}

#mask{ display: none; width: 100%; height: 1087px; position: absolute; z-index: 1; background: #949494; /*filter:alpha(opacity=50); -moz-opacity:0.5; -khtml-opacity: 0.5;*/  opacity: 0.5}

#form-contain{ display: none; border-radius: 10px; width: 810px; height: 668px; background: #fff; position: absolute; left: 50%; top: 50%; margin-top: -260px; margin-left: -405px; z-index: 10;}
#form{ width: 440px; height: 375px; margin: 0 auto;}
#form-p{ text-align: center; font-size: 18px; margin-top: 36px;}
#form-table{ margin-top: 36px; width: 440px; height: 260px; font-size: 14px;}
.th1{ text-align: right; width: 86px; vertical-align: top; padding: 9px; }
.th1 p{ display: inline-block; font-weight: normal; font-size: 14px;}
.th2{ padding-left: 26px; width: 280px; height: 30px;}
.th2 input{ width: 258px; height: 28px; font-size: 12px; padding: 0 10px; border:#efefef 1px solid;}
.th2 textarea{ width: 258px; height: 80px; font-size: 12px; padding: 10px 10px; border:#efefef 1px solid;}
#form-button{ width:440px; height: 34px;}
#form-button a{ display: block; width: 115px; height: 34px; margin: 0 auto; background: url(../images/submit.png);}

#contact-contain{ display: none; width: 810px; height: 268px; background: #f1f1f1; padding-top: 10px; border-bottom-left-radius: 10px; border-bottom-right-radius: 10px;}
#contact{ width: 440px; height: 200px; margin: 20px auto 0 auto; background: #f1f1f1; }
#contact-p{ text-align: center; font-size: 18px;}
#contact-table{ width: 228px; height: 158px; margin: 18px auto 0 auto; font-size: 14px; text-align: left;}
/*#contact-ul li p{ display: inline-block; font-size: 14px;} 
#contact-ul li img{ margin: 5px 13px 5px 0;}*/
#contact table{ margin-top: 18px;}
#contact table tr th img{ margin: 5px 13px 5px 0;}